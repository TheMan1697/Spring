 <mapper namespace="works.yermi.mapper.RoomMapper">
 	<select id="getList" resultType="room">
 		SELECT * 
		FROM TBL_ROOM WHERE PENSIONID = #{pensionid}
		
 	</select>
 	
 	<select id="read" resultType="room">
 		SELECT *
 		FROM TBL_ROOM
 		WHERE ROOMNUM = #{roomNum}
 	</select>
 
 </mapper> 